<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.tk.car">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.tk.car.LoginActivity"
            android:parentActivityName="com.tk.car.SelectOptionAuthActivity"
            ></activity>
        <activity android:name="com.tk.car.RegisterActivity"
            android:parentActivityName="com.tk.car.SelectOptionAuthActivity"/>
        <activity android:name="com.tk.car.SelectOptionAuthActivity"
            android:parentActivityName="com.tk.car.MainActivity"/>
        <activity android:name="com.tk.car.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>